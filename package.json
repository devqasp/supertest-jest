{
    "name": "supertest_jest",
    "version": "1.0.0",
    "description": "This is a API testing framework using Supertest and Jest",
    "main": "index.js",
    "scripts": {
        "test": "jest"
    },
    "author": "Prasad Mudedla",
    "license": "ISC",
    "devDependencies": {
        "jest": "^23.4.1",
        "jest-cli": "^23.4.1",
        "jest-html-reporter": "^2.4.1",
        "superagent": "^3.8.3",
        "superagent-retry": "^0.6.0",
        "supertest": "^3.1.0"
    },
    "jest": {
        "testEnvironment": "node",
        "testPathIgnorePatterns": [
            "/node_modules/"
        ],
        "reporters": [
            "default", ["./node_modules/jest-html-reporter", {
                "pageTitle": "Test Report",
                "outputPath": "Report/index.html",
                "includeFailureMsg": true
            }]
        ]
    }
}
